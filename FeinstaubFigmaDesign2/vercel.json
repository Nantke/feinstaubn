{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build"
    }
  ],

  "rewrites": [
    {
      "source": "/api/index",
      "destination": "/api/index.js"
    },
    {
      "source": "/api/umwelt/:path*",
      "destination": "https://www.umweltbundesamt.de/api/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
